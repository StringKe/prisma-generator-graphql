{
  "name": "@stringke/prisma-generator-graphql",
  "description": "A generator graphql schema from prisma schema",
  "version": "1.0.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-graphql": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "pnpm build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "3.12.0",
    "@prisma/generator-helper": "3.12.0",
    "@prisma/sdk": "3.12.0",
    "prettier": "2.8.8"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "27.0.3",
    "@types/node": "20.2.1",
    "@types/prettier": "2.4.2",
    "jest": "27.4.7",
    "prisma": "3.12.0",
    "semantic-release": "^18.0.1",
    "ts-jest": "27.1.4",
    "typescript": "4.6.2"
  },
  "homepage": "https://github.com/StringKe/prisma-generator-graphql",
  "repository": {
    "type": "git",
    "url": "https://github.com/StringKe/prisma-generator-graphql.git"
  },
  "author": "stringke <authemail@qq.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
